    "import theano.tensor as T\n",
    "import theano\n",
    "\n",
    "a = T.vector()\n",
    "b = T.matrix()\n",
    "fa = a ** 2\n",
    "f = theano.function([a], fa)\n",
    "f2 = theano.function([b], theano.clone(fa, replace={a: b}, strict=False))\n",
    "print(f([2]))\n",
    "print(f2([[2]]))"
   ]
